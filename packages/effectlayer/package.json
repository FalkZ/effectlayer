{
    "name": "effectlayer",
    "version": "0.0.2",
    "type": "module",
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build",
        "prepublishOnly": "bun run build && cp ../../README.md ./README.md"
    },
    "exports": {
        ".": {
            "import": "./dist/effectlayer.js",
            "types": "./dist/effectlayer.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/jsx-runtime.js",
            "types": "./dist/jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/jsx-runtime.js",
            "types": "./dist/jsx-runtime.d.ts"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "devDependencies": {
        "@total-typescript/tsconfig": "^1.0.4",
        "@types/node": "^24.7.2",
        "typescript": "~5.9.3",
        "vite": "^7.1.7",
        "vite-bundle-analyzer": "^1.2.3",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-replace": "^0.1.1",
        "dev-assert": "workspace:*"
    },
    "dependencies": {
        "@preact/signals-core": "^1.12.1",
        "@types/react": "^19.2.2",
        "fast-deep-equal": "^3.1.3",
        "hyperx": "^3.0.1",
        "immer": "^10.1.3",
        "is-boolean-attribute": "^0.0.1",
        "property-information": "^7.1.0",
        "snabbdom": "3.6.3",
        "type-fest": "^5.0.1"
    }
}
